{
  "name": "CSV-SE",
  "displayName": "Chrubeanie_Source_Visualizer",
  "description": "A Source Code Visualizer for Software Engineering One Class",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.94.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "commands": [{
      "command": "CSV-SE.showSourceVisualizer",
      "title": "SE Source Visualizer(Chrubeanie)"
    }],
    "views": {
      "explorer": [
        {
          "id": "SourceTree",
          "name": "Source Tree List"
        }
      ]
    }
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/vscode": "^1.94.0",
    "@types/mocha": "^10.0.8",
    "@types/node": "20.x",
    "eslint": "^9.11.1",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1"
  }
}

<!doctype html>
<html lang="en">
    <head>
        <title>Source Code Visualizer</title>
        <!-- Required meta tags -->
        <meta charset="utf-8" />
        <meta
            name="viewport"
            content="width=device-width, initial-scale=1, shrink-to-fit=no"
        />
        <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
        <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz" crossorigin="anonymous"></script>
    </head>

    <body class="container-fluid" data-bs-theme="dark">
        <header>
                <nav class="navbar navbar-expand-sm navbar-light bg-primary">
                    <div class="container-fluid">
                        <h1 class="navbar-brand text-decoration-none">Source Code Visualizer: Chrubeanie</h1>
                            <ul class="navbar-nav border justify-content-evenly">
                                <button class="btn btn-dark">Previous</button>
                                <button class="btn btn-dark">Next</button>
                                <button class="btn btn-dark">Save</button>
                            </ul>
                        </div>
                </nav>
            
        </header>
        <main>
            <div class="container-fluid justify-content-center align-items-center">
                <div class="row justify-content-center align-items-center g-2">
                    <div class="col">
                        <div class="table-responsive">
                            <table class="table table-primary">
                                <thead>
                                    <h6>FileName</h6>
                                    <tr>
                                        <th scope="col">Classes</th>
                                        <th scope="col">Methods</th>
                                        <th scope="col">Variables</th>
                                        <th scope="col">IDK YET</th>
                                    </tr>
                                </thead>
                                <tbody>
                                    <% for(let i = 0; i < data.length; i++) { %>
                                    <tr>
                                            <th scope="row"><%= i + 1 %></th>
                                            <td><%= data[i].first %></td>
                                            <td><%= data[i].last %></td>
                                            <td><%= data[i].handle %></td>
                                        </tr>
                                    <% } %>
                                </tbody>
                            </table>
                        </div>
                        
                    </div>
                </div>
            </div>
            <br>
            <br>
            <table class="table">
                <thead>
                    <tr>
                        <th scope="col">#</th>
                        <th scope="col">First</th>
                        <th scope="col">Last</th>
                        <th scope="col">Handle</th>
                    </tr>
                </thead>
              
            </table>
        </main>
        <footer>
            <!-- place footer here -->
        </footer>
        <!-- Bootstrap JavaScript Libraries -->
        <script src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.11.8/dist/umd/popper.min.js" integrity="sha384-I7E8VVD/ismYTF4hNIPjVp/Zjvgyol6VFvRkX/vR+Vc4jQkC+hVqc2pM8ODewa9r" crossorigin="anonymous"></script>
        <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.min.js" integrity="sha384-0pUGZvbkm6XF6gxjEnlmuGrJXVbNuzT9qBBavbLwCsOGabYfZo0T0to5eqruptLy" crossorigin="anonymous"></script>
    </body>
</html>
